class Doughnut {
    cook() {
        print "Fry " + this.name() + " until golden brown.";
    }

    name() {
        return "Doughnut";
    }
}

class BostonCream < Doughnut {
    cook() {
        super.cook();
        print "Pipe full of custard and coat with chocolate.";
    }
    name() {
        return "BostonCream";
    }
}

BostonCream().cook();